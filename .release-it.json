{
  "dry-run": true,
  "dryRun": true,
  "git": {
    "changelog": "node release.js ${from} ${to} ${repo}",
    "requireCleanWorkingDir": false,
    "commit": true,
    "commitMessage": "Release ${version}",
    "tag": true,
    "tagAnnotation": "Release ${version}",
    "push": true,
    "pushArgs": ["--follow-tags"]
  },
  "github": {
    "release": true,
    "releaseName": "Release ${version}"
  },
  "npm": {
    "publish": false
  },
  "gitlab": {
    "release": false
  }
}
